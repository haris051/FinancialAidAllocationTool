@model FinancialAidAllocationTool.Models.IntakeSeason.FaatIntakeSeason

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body>

<h4>FaatIntakeSeason</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Year" class="control-label"></label>
                <input asp-for="Year" class="form-control" />
                <span asp-validation-for="Year" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IntakeSeason" class="control-label"></label>
                <input asp-for="IntakeSeason" class="form-control" />
                <span asp-validation-for="IntakeSeason" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="InsertionTimestamp" class="control-label"></label>
                <input asp-for="InsertionTimestamp" class="form-control" />
                <span asp-validation-for="InsertionTimestamp" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
@Html.ActionLink("Back To List", "index","CloseScholarship",new {Type=TempData["Type"]})
</div>

</body>
</html>
